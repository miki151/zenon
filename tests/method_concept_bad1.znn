// no_compile

concept<T, U> CanConvert {
  U convert(T* t);
};

template <T>
struct S1 {
  T t;
};

template <T, U> requires CanConvert<U, T>
void set(S1<T> mutable* s, U u) {
  s->t = u.convert();
}

struct X {
  int a;
};

int convert(X* x) {
  return 1 + x->a;
}

int main() {
  mutable s1 = S1(1);
  s1.set(5);
  return s1.t;
}
