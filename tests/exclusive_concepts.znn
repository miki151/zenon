// 3

template<T>
concept c1 {
  int fun1(T*);
};

template<T>
concept c2 {
  int fun2(T*);
};

template <T> requires c1<T>
int f(T t) {
  return t.fun1();
}

template <T> requires c2<T>
int f(T t) {
  return t.fun2();
}

struct X {
};

int fun1(X*) {
  return 1;
}

struct Y {
};

int fun2(Y*) {
  return 2;
}

int main() {
  return f(X()) + f(Y());
}
