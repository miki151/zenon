// 6

template <Fun, Ret, Args...>
concept can_invoke2 {
  Ret invoke(Fun*, Args...);
  void destruct(Fun*);
};

template <T>
int get(can_invoke2<int, T> fun, T t) {
  return fun.invoke(move(t));
}

int main() {
  const lam = [](string s) { return s.size(); };
  const fun = cast<can_invoke2<int, string>>(move(lam));
  return get(move(fun), "pokpok");
}
