import "std/box.znn";

concept <Fun, Ret, Args...> invoke2 {
  Ret invoke(Fun*, Args...);
  void destruct(Fun*);
};

template <T>
int get(box<invoke2<int, T>> fun, T t) {
  return fun->invoke(move(t));
}

int main() {
  const lam = [](string s) { return s.size(); };
  const fun = move(lam).box_as<invoke2<int, string>>();
  return get(move(fun), "pokpok");
}
