// 9

concept <T> C1 {
  int get(T*);
  void set(T mutable*, int);
};

struct X {
  int a;
};

int get(X* x) {
  return x->a;
}

void set(X mutable* x, int a) {
  x->a = a;
}

int main() {
  mutable x = X(5);
  const C1 mutable* c = &x;
  const res = c->get();
  c->set(4);
  return c->get() + res;
}
