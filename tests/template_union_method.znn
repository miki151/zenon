// 4

template <T> requires can_copy<T>
union V {
  T value;
  void isNull;
};

template <T> requires can_copy<T>
T getValueOr(V<T>* v, T a) {
  switch (*v) {
    case (value) {
      return value.copy();
    }
    case (isNull) {
      return move(a);
    }
  }
}

int main() {
  mutable v = V<int>::value(5);
  v = V<int>::isNull();
  return v.getValueOr(4);
}
