template <T>
int get(T) {
  return 0;
}

int get(int a) {
  return a;
}

int sum() {
  return 0;
}

template <Arg, Args...>
int sum(Arg a, Args... args) {
  mutable i = 0;
  for (k = get(move(a)) + sum(move(args)...); k > 0; --k)
    ++i;
  return i;
}

int main() {
  return sum(1, 2, 'a', 3);
}
