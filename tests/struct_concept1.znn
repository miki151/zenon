// 10

concept<T> CanAdd {
  T operator + (T* t1, T* t2);
};

template <A>
struct S1 {
  A a;
};

template <A> requires CanAdd<A>
A get(S1<A>* s1) {
  return s1->a + s1->a;
}

int main() {
  const s = S1(5);
  const s2 = S1("pok");
  if (s2.get() == "pokpok")
    return s.get();
  else
    return 0;
}
