// 16

concept<T> C1 {
  int call(T*);
};

struct X {
  int a;
};

int call(X* x) {
  return x->a;
}

struct Y {
};

int call(Y*) {
  return 10;
}

int main() {
  const x = X(5);
  mutable res = 1;
  mutable C1* c = &x;
  res += c->call();
  const y = Y();
  c = &y;
  res += call(c);
  return res;
}
