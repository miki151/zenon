// 3

embed {
#include <memory>
using std::unique_ptr;
}


template <T>
extern struct unique_ptr {
};

template <T>
T mutable* get(unique_ptr<T> mutable* ptr) {
  embed {
    return ptr->get();
  }
}

template <T>
T* get(unique_ptr<T>* ptr) {
  embed {
    return ptr->get();
  }
}


template <T>
unique_ptr<T> unique(T t) {
  embed {
    return std::make_unique<T>(std::move(t));
  }
}

int main() {
  mutable ptr = unique(3);
  const ptr2 = move(ptr);
  return *ptr2.get();
}
