// no_compile

int get_value(string* s) { return s->size(); }
int get_value(int* a) { return *a; }

concept <T> has_value {
  int get_value(T*);
};

template <Args...> requires has_value<Args>...
int g(Args*... args) {
  mutable res = 0;
  static for (i = 0; i < countof(args); ++i)
    res += get_value(args...[i + 1]);
  return res;
}

int main() {
  return g(&1);
}
