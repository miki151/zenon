// 11

template <T>
concept C1 {
  int get(T*);
  void set(T mutable*, int);
};

struct X {
  int a;
};

int get(X* x) {
  return x->a;
}

void set(X mutable* x, int a) {
  x->a = a;
}

int f(C1 c) {
  return c.get();
}

int main() {
  const x = X(5);
  mutable c = cast<C1>(move(x));
  const res1 = c.get();
  c.set(6);
  return res1 + f(move(c));
}
