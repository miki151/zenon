// 15

template<T>
void set(T mutable* x, mutable T y) {
  *x = move(y);
}

struct X {
  int* a;
  int b;
};

int main() {
  mutable x = 0;
  mutable a = true;
  const y = &x;
  (x) = 1;
  set(y, 5);
  set(&a, false);
  mutable x1 = X(&x, *y);
  const x2 = &x1;
  x2->a = y;
  *y = -x2->b + 20;
  return *x2->a;
}
