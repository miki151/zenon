// 3

import "std/box.znn";

struct X {
  int mutable* cnt;
};

void destruct(X* x) {
  ++*x->cnt;
}

struct Y {
  box<X[2]> x;
};

struct Z {
  Y y;
  box<X> x;
};

template <T>
void f(T) {}

int main() {
  mutable a = 0;
  {
    const y = Z(Y(box({X(&a), X(&a)})), box(X(&a)));
    f(move(y));
  }
  return a;
}
