// 2

struct X {
  int a;
};

X operator + (X* a, X* b) {
  return X(a->a + b->a);
}

X operator - (X* a, X* b) {
  return X(a->a - b->a);
}

X operator / (X* a, X* b) {
  return X(a->a / b->a);
}

X operator * (X* a, X* b) {
  return X(a->a * b->a);
}

int main() {
  mutable x = X(1);
  x += X(4);
  x -= X(2);
  x *= X(2);
  x /= X(3);
  return x.a;
}
