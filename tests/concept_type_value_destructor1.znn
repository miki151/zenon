template <T>
concept C1 {
  int get(T*);
  void destruct(T*);
};

struct X {
  int mutable* cnt;
};

void destruct(X* x) {
  ++*x->cnt;
}

int get(X* x) {
  return 1;
}

int f(C1 c) {
  return c.get();
}

int main() {
  mutable cnt = 1;
  {
    const x = X(&cnt);
    const y = X(&cnt);
    mutable C1 c = move(x);
    cnt += f(move(c));
  }
  return cnt;
}
